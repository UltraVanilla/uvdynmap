{
  "name": "uvdynmap",
  "version": "1.0.0",
  "description": "Dynmap extensions for UltraVanilla",
  "scripts": {
    "start": "node lib/server.js",
    "test": "node lib/test.js",
    "build-ts": "tsc --declaration",
    "watch-ts": "tsc -w --declaration",
    "bundle": "browserify lib/browser.js -o assets/ultravanilla.js",
    "watch-bundle": "watchify lib/browser.js -o assets/ultravanilla.js --verbose --debug",
    "grab-vendor-assets": "node build.js",
    "build": "npm run build-ts && npm run bundle && npm run grab-vendor-assets",
    "install": "npm run build"
  },
  "author": "lordpipe",
  "license": "(CC0-1.0 OR ISC)",
  "dependencies": {
    "argon2": "^0.27.0",
    "cheerio": "^1.0.0-rc.3",
    "knex": "^0.21.5",
    "koa": "^2.13.0",
    "koa-mount": "^4.0.0",
    "koa-proxies": "^0.11.0",
    "koa-router": "^9.4.0",
    "koa-static": "^5.0.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.0",
    "pkg-dir": "^4.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/jquery": "^3.5.1",
    "@types/koa": "^2.11.3",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-proxy": "^1.0.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "browserify": "^16.5.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "get-google-fonts": "^1.2.2",
    "jspanel4": "^4.11.0-beta",
    "jsx-dom": "^6.4.19",
    "prettier": "^2.1.2",
    "typed-html": "^2.0.0",
    "typescript": "^3.9.7",
    "watchify": "^3.11.1"
  }
}
