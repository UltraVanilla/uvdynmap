{
  "name": "uvdynmap",
  "version": "1.0.0",
  "description": "Dynmap extensions for UltraVanilla",
  "scripts": {
    "start": "node lib/server.js",
    "test": "node lib/test.js",
    "ts": "tsc --declaration",
    "watch": "run-p watch:* watch:bundle:*",
    "watch:ts": "tsc -w --declaration",
    "watch:bundle:dynmap": "watchify lib/browser-dynmap.js -o assets/dynmap.js --verbose --debug",
    "watch:bundle:coreprotect-tools": "watchify lib/browser-coreprotect-tools.js -o assets/coreprotect-tools.js --verbose --debug",
    "bundle": "run-p bundle:*",
    "bundle:dynmap": "browserify lib/browser-dynmap.js -o assets/dynmap.js",
    "bundle:coreprotect-tools": "browserify lib/browser-coreprotect-tools.js -o assets/coreprotect-tools.js",
    "grab-vendor-assets": "node build.js",
    "build": "npm run ts && npm run bundle && npm run grab-vendor-assets",
    "install": "npm run build"
  },
  "author": "lordpipe",
  "license": "(CC0-1.0 OR ISC)",
  "dependencies": {
    "argon2": "^0.27.0",
    "cheerio": "^1.0.0-rc.3",
    "knex": "^0.21.5",
    "koa": "^2.13.0",
    "koa-mount": "^4.0.0",
    "koa-proxies": "^0.11.0",
    "koa-router": "^9.4.0",
    "koa-static": "^5.0.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.0",
    "pkg-dir": "^4.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/codemirror": "0.0.98",
    "@types/jquery": "^3.5.1",
    "@types/koa": "^2.11.3",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-proxy": "^1.0.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "browserify": "^16.5.2",
    "codemirror": "^5.58.1",
    "date-fns": "^2.16.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "get-google-fonts": "^1.2.2",
    "jspanel4": "^4.11.0-beta",
    "jsx-dom": "^6.4.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "typed-html": "^2.0.0",
    "typescript": "^3.9.7",
    "watchify": "^3.11.1"
  }
}
